{
    "version": 2,
    "builds": [
      {
        "src": "*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "functions": {
      "controllers/*.js": {
        "maxDuration": 60
      },
      "middlewares/*.js": {
        "maxDuration": 60
      }
    }
  }
